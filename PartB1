public class Main {
    public static void main(String[] args)
    { // provides 5 lines
        for (int i = 1; i <= 5; i++)
        { // add * for the number line its on
            for (int s = 1; s <= i; s++) {
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
