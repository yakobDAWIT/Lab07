public class Main {
    public static void main(String[] args)
    { // provides 5 lines
        for (int i = 1; i <= 5; i++)
        { // subtract 1 * each line starting from 5 *
            for (int s = 5; s >= i; s--) {
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
